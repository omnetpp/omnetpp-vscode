{
  "name": "omnetpp-vscode",
  "displayName": "OMNeT++ NED and MSG",
  "description": "Official extension for the OMNeT++ NED and MSG languages",
  "version": "0.0.4",
  "publisher": "opensim",
  "repository": "https://github.com/omnetpp/omnetpp-vscode",
  "icon": "logo128.png",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "package": "vsce package",
    "deploy": "vsce publish"
  },
  "contributes": {
    "languages": [
      {
        "id": "OMNeT++ NED",
        "extensions": [
          ".ned"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "OMNeT++ MSG",
        "extensions": [
          ".msg"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "OMNeT++ NED",
        "scopeName": "source.ned",
        "path": "./syntaxes/ned.tmLanguage.json"
      },
      {
        "language": "OMNeT++ MSG",
        "scopeName": "source.msg",
        "path": "./syntaxes/omnetpp-msg.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.0.0"
  }
}
